      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "pic = np.sin(x).reshape(len(x), 1)  * np.cos(x)\n",
    "pt.imshow(pic)"
